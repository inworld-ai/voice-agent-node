# Voice Agent Server (templates/voice_agent/voice_agent_server)
#
# Copy this file to `.env` in the same folder and fill values.
#

# --- Required for ALL modes ---
INWORLD_API_KEY=

# --- Optional defaults ---
VOICE_ID=Dennis
DISABLE_AUTO_INTERRUPTION=false

# LLM/TTS settings used by AssemblyAI graph path
LLM_PROVIDER=groq
LLM_MODEL_NAME=llama-3.3-70b-versatile
TTS_MODEL_ID=inworld-tts-1

# --- Native RealtimeAgent mode (sttService="native") ---
# If set, RealtimeAgent uses Groq STT; otherwise it uses Inworld STT.
GROQ_API_KEY=

# Native audio pipeline local models (recommended)
VAD_MODEL_PATH=
TURN_DETECTOR_MODEL_PATH=

# --- AssemblyAI mode (sttService="assemblyai") ---
ASSEMBLY_AI_API_KEY=


